{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [ "es2017", "dom"],
    
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
